{
  "manifest_version": 2,

  "name": "PSI Screen",
  "description": "This extension shows the PSI for the selected Area",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ],

  "content_security_policy": "script-src 'self' https://maps.googleapis.com/ 'unsafe-eval' ; object-src 'self'"
}
