{
  "manifest_version": 2,

  "name": "PSI Screen",
  "description": "This extension shows the PSI for the selected Area",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://maps.googleapis.com/maps/api/*"
  ]
}
